{
  "type": "object",
  "required": [
    "external_uid",
    "content",
    "author",
    "created_at",
    "ticketable_attributes",
    "personal_access_token",
    "external_link"
  ],
  "properties": {
    "external_uid": {
      "type": "string",
      "examples": [
        "123hello321world"
      ]
    },
    "content": {
      "type": "string",
      "examples": [
        "This is content from an example external ticket."
      ]
    },
    "parent_uid": {
      "type": "string",
      "examples": [
        "external_ticket_parent_uid"
      ]
    },
    "author": {
      "type": "object",
      "required": [
        "external_uid",
        "external_link",
        "username"
      ],
      "properties": {
        "external_uid": {
          "type": "string",
          "examples": [
            "external_ticket_author_id"
          ]
        },
        "external_link": {
          "type": "string",
          "examples": [
            "https://linkedin.com/company/test-1337"
          ]
        },
        "username": {
          "type": "string",
          "examples": [
            "best_username_ever"
          ]
        }
      }
    },
    "created_at": {
      "type": "string",
      "examples": [
        "2020-04-04 23:35:27.632879 UTC"
      ]
    },
    "ticketable_attributes": {
      "type": "object",
      "required": ["response_url"],
      "properties": {
        "response_url": {
          "type": "string",
          "examples": [
            "https://response_url.com"
          ]
        },
        "custom_provider": {
          "type": "string",
          "examples": [
            "hacker_news"
          ]
        },
        "additionalProperties": false
      }
    },
    "personal_access_token": {
      "type": "object",
      "required": [
        "name",
        "token"
      ],
      "properties": {
        "name": {
          "type": "string",
          "examples": [
            "token_name"
          ]
        },
        "token": {
          "type": "string",
          "examples": [
            "123TOKEN321"
          ]
        }
      }
    },
    "external_link": {
      "type": "string",
      "examples": [
        "https://www.linkedin.com/feed/update/urn:li:share:7122658645678465024"
      ]
    }
  },
  "additionalProperties": false
}
